print("3出现了",list.count(3),"次")
print("4出现了",list.count(4),"次")
print("5出现了",list.count(5),"次")
print("6出现了",list.count(6),"次")
print("7出现了",list.count(7),"次")
print("8出现了",list.count(8),"次")
print("9出现了",list.count(9),"次")
print("10出现了",list.count(10),"次")
